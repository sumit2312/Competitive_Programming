//https://www.hackerrank.com/challenges/permutation-equation/problem
#include "bits/stdc++.h"
using namespace std;

#define ll long long
#define int ll

signed main(){
    int n;
    cin>>n;
    int a[n+1];
    for(int i=1;i<n+1;i++){
        int k;
        cin>>k;
        a[k]=i;
    }
    for(int i=1;i<=n;i++){
        cout<<a[a[i]]<<endl;
    } 
}
